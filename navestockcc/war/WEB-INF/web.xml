<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

<welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
</welcome-file-list>
    
    <servlet>
        <servlet-name>teamListServelet</servlet-name>
        <servlet-class>org.navestock.teams.teamListServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>teamListServelet</servlet-name>
        <url-pattern>/team.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>createMatchServelet</servlet-name>
        <servlet-class>org.navestock.match.createMatchServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>createMatchServelet</servlet-name>
        <url-pattern>/CreateMatch.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>matchListServelet</servlet-name>
        <servlet-class>org.navestock.match.matchListServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>matchListServelet</servlet-name>
        <url-pattern>/matchlist.do</url-pattern>
    </servlet-mapping>
    
    
    
</web-app>